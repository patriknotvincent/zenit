<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane id="anchor" prefHeight="203.0" prefWidth="348.0">
         <children>
            <TextField fx:id="tfName" layoutX="10.0" layoutY="65.0" prefHeight="27.0" prefWidth="327.0" promptText="New folder name" />
            <Button layoutX="270.0" layoutY="111.0" mnemonicParsing="false" onAction="#browse" prefHeight="27.0" prefWidth="68.0" text="Browse" />
            <Button layoutX="196.0" layoutY="165.0" mnemonicParsing="false" onAction="#create" prefHeight="25.0" prefWidth="68.0" text="Create" />
            <Button layoutX="272.0" layoutY="165.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="68.0" text="Cancel" />
            <Separator layoutY="152.0" prefHeight="9.0" prefWidth="348.0" />
            <ListView fx:id="filepath" layoutX="10.0" layoutY="110.0" prefHeight="28.0" prefWidth="252.0" />
         </children>
      </Pane>
      <AnchorPane id="setup-header" fx:id="header" prefHeight="61.0" prefWidth="348.0">
         <children>
            <ImageView fx:id="logo" fitHeight="45.0" fitWidth="45.0" layoutX="289.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" />
            <Text fill="#515151" layoutX="21.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create new folder">
               <font>
                  <Font name="Droid Sans Mono" size="24.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
