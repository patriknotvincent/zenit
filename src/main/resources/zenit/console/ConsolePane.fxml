<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.zenit.console.ConsoleController">
   <children>
      <AnchorPane fx:id="rootAnchor" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0" />
      <AnchorPane minHeight="20.0" prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: ddd;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button id="btnConsole" fx:id="btnConsole" layoutX="23.0" layoutY="5.0" onMouseClicked="#showConsoleTabs" text="console">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <Button id="btnTerminal" fx:id="btnTerminal" layoutX="99.0" layoutY="5.0" onMouseClicked="#showTerminalTabs" text="terminal">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <ChoiceBox fx:id="consoleChoiceBox" nodeOrientation="LEFT_TO_RIGHT" prefWidth="90.0" style="-fx-background-color: rgba(255,255,255,0);" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <ChoiceBox fx:id="terminalChoiceBox" nodeOrientation="LEFT_TO_RIGHT" prefWidth="90.0" style="-fx-background-color: rgba(255,255,255,0);" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <!-- <opaqueInsets>
                  <Insets />
               </opaqueInsets> -->
               </ChoiceBox>
            <Button fx:id="btnNewConsole" layoutX="181.0" mnemonicParsing="false" onMouseClicked="#startNewConsole" style="-fx-background-color: rgba(255,255,255,0);" text="new console" AnchorPane.leftAnchor="181.0" />
            <Button fx:id="btnNewTerminal" layoutX="181.0" mnemonicParsing="false" onMouseClicked="#newTerminal" style="-fx-background-color: rgba(255,255,255,0);" text="new terminal" AnchorPane.leftAnchor="181.0"/>
            <Button fx:id="btnClear" layoutX="500.0" mnemonicParsing="false" onMouseClicked="#clearConsole" style="-fx-background-color: transparent;" text="clear" AnchorPane.rightAnchor="158.4">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
